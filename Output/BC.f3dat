;----------------------------------
;SET GRAVITY
;----------------------------------
;set gravity 0,0,-32
;ini density 1000.0
;----------------------------------
;DEFINE FIX CONDITION
;----------------------------------
fix x range x -100.1 -99.9
fix x range x 99.9 100.1
fix y range y -100.1 -99.9
fix y range y 99.9 100.1
fix z range z -1557.1 -1556.9
 
;----------------------------------
;INITIAL STRESS
;----------------------------------
;ini szz -1740.0 
;ini sxx -435.0 
;ini syy -435.0 
 
;ini szz -1 grad 0,0,1.2
;ini sxx -1 grad 0,0,0.2
;ini syy -1 grad 0,0,0.2
 
;----------------------------------
;APPLY VERTICAL STRESS
;----------------------------------
apply szz -1740.0 range z -1350.11,-1349.9
 
